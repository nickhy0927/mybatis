<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iss.module.platform.role.mapper.RoleMapper">
	<select id="queryRoleList" parameterType="String" resultType="com.iss.module.platform.role.entity.Role">
		select * from sys_role t ,sys_user_role su where su.role_id = t.id and su.user_id = #{userId}
	</select>
	<select id="queryPageByMap" resultType="com.iss.module.platform.user.pojo.User">
		select * from sys_user t where t.status = 1
	</select>
</mapper>